{
  "name": "remix-starter",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#app/*": "./app/*",
    "#tests/*": "./tests/*"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix vite:build",
    "build:server": "tsx ./other/build-server.ts",
    "dev": "node ./server/dev-server.js",
    "lint": "eslint .",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "prepare": "husky",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "test:e2e": "npm run test:e2e:dev --silent",
    "test:e2e:dev": "playwright test --ui",
    "test:e2e:run": "cross-env CI=true playwright test",
    "typecheck": "tsc",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,css}": "prettier --ignore-path .gitignore --write"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@remix-run/express": "^2.14.0",
    "@remix-run/node": "^2.14.0",
    "@remix-run/react": "^2.14.0",
    "address": "^2.0.3",
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.0",
    "close-with-grace": "^2.1.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.1",
    "framer-motion": "^11.11.13",
    "get-port": "^7.1.0",
    "glob": "^11.0.0",
    "helmet": "^8.0.0",
    "isbot": "^4.1.0",
    "lucide-react": "^0.457.0",
    "morgan": "^1.10.0",
    "prisma": "^5.22.0",
    "react": "18.3.0-canary-a870b2d54-20240314",
    "react-dom": "18.3.0-canary-a870b2d54-20240314",
    "remix-auth": "^3.7.0",
    "remix-auth-form": "^1.5.0",
    "remix-auth-github": "^2.0.0",
    "remix-starter": "link:",
    "remix-utils": "^7.7.0",
    "source-map-support": "^0.5.21",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
