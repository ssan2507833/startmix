    "@types/eslint": "^9.6.1",
    "@types/express": "^4.17.20",
    "@types/fs-extra": "^11.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.0",
    "@types/react": "^18.2.20",
